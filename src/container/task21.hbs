<div>
    <main>
        <div class="card">
            <div class="card-body">
                {{#with heading}}
                <section>
                    {{#with main}}
                    <div class="card">
                        <div class="card-body">
                            <p>
                            <h1> {{title}} </h1>
                            </p>
                            <p> {{text}} </p>
                            {{#with button_text}}
                            <button class="btn btn-primary"> {{solid}} </button>
                            <br>
                            <br>
                            <br>
                            <button class="btn btn-outline-primary"> {{outline}} </button>
                            {{/with}}
                        </div>
                    </div>
                    {{/with}}
                </section>

                <section>
                    {{#with second}}
                    <div class="card">
                        <div class="card-body">
                            <p>
                            <h2> {{title}} </h2>
                            </p>
                            <p> {{text}} </p>
                            {{#with button_text}}
                            <button class="btn btn-secondary"> {{solid}} </button>
                            <br>
                            <br>
                            <button class="btn btn-outline-secondary"> {{outline}} </button>
                            {{/with}}
                        </div>
                    </div>
                    {{/with}}
                </section>

                <section>
                    {{#with subblock}}
                    <div class="card">
                        <div class="card-body">
                            <p>
                            <h3> {{title}} </h3>
                            </p>
                            <p> {{text}} </p>
                            {{#with button_text}}
                            <button class="btn btn-dark"> {{solid}} </button>
                            <br>
                            <br>
                            <button class="btn btn-outline-dark"> {{outline}} </button>
                            {{/with}}
                        </div>
                    </div>
                    {{/with}}
                </section>
                {{/with}}
            </div>
        </div>

        <br>
        <br>

        <section>
            <div class="card">
                <div class="card-body">
                    {{#with nested_display}}
                    <p class="display-1"> {{title}} </p>
                    <div class="card">
                        <div class="card-body">
                            {{#with children_display}}
                            <p class="display-4"> {{title}} </p>
                            <p> {{text}} </p>
                            <button class="btn btn-outline-warning"> {{button.text}} </button>
                            {{/with}}
                        </div>
                    </div>
                    <p>
                    <h6> {{header_text}} </h6>
                    </p>
                    <button class="btn btn-info"> {{button.text}} </button>
                    {{/with}}
                </div>
            </div>
        </section>

        <section>
            <div class="card">
                <div class="card-body">
                    {{#with paragraph}}
                    <p class="text-uppercase"> {{title}} </p>
                    <p class="text-lowercase"> {{text}} </p>
                    {{#with button}}
                    <a href="{{url}}" class="btn btn-link"> {{text}} </a>
                    {{/with}}

                    <div class="card">
                        <div class="card-body">
                            {{#each list_name}}
                            <ul>
                                <li>
                                    <a href="{{url}}" class="btn btn-link"> {{text}} </a>
                                </li>
                            </ul>
                            {{/each}}
                        </div>
                    </div>
                    {{/with}}
                </div>
            </div>
        </section>
    </main>
</div>